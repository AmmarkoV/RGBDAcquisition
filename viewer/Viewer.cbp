<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Viewer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="./Viewer" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="-module OPENNI2 -from ../redist/ACapturedCalibration.oni " />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="./Viewer" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="`pkg-config --cflags opencv`" />
			<Add option="-fPIC" />
		</Compiler>
		<Linker>
			<Add option="`pkg-config --libs opencv`" />
			<Add library="GL" />
			<Add library="GLU" />
			<Add library="X11" />
			<Add library="../acquisition/libRGBDAcquisition.so" />
			<Add library="rt" />
			<Add library="../tools/Calibration/libCalibrationLibrary.a" />
			<Add library="../tools/Timers/libTimers.a" />
			<Add library="dl" />
			<Add library="../tools/OperatingSystem/libOperatingSystem.a" />
		</Linker>
		<Unit filename="../tools/Common/viewerSettings.h" />
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
