<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Grabber" />
		<Option pch_mode="2" />
		<Option default_target="Release" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="./Grabber" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="-module OPENNI1 -from test -to /dev/null -maxFrames 100 " />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="./Grabber" prefix_auto="0" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="-module TEMPLATE -from test -to /dev/null -maxFrames 100 " />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fPIC" />
		</Compiler>
		<Linker>
			<Add library="GL" />
			<Add library="GLU" />
			<Add library="X11" />
			<Add library="../acquisition/libAcquisition.so" />
			<Add library="rt" />
			<Add library="../tools/Calibration/libCalibrationLibrary.a" />
			<Add library="../tools/Timers/libTimers.a" />
			<Add library="dl" />
			<Add library="../tools/Filesystem/libFilesystemListing.a" />
		</Linker>
		<Unit filename="main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
